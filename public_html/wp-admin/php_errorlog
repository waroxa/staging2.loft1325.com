            (SELECT rendered_subject FROM lum_loft_email_renders r WHERE r.job_id = j.id ORDER BY r.id DESC LIMIT 1) AS rendered_subject,
            (SELECT rendered_body FROM lum_loft_email_renders r WHERE r.job_id = j.id ORDER BY r.id DESC LIMIT 1) AS rendered_body,
            (SELECT rendered_text FROM lum_loft_email_renders r WHERE r.job_id = j.id ORDER BY r.id DESC LIMIT 1) AS rendered_text,
            (SELECT variables FROM lum_loft_email_renders r WHERE r.job_id = j.id ORDER BY r.id DESC LIMIT 1) AS render_variables
        FROM lum_loft_email_jobs j
        LEFT JOIN lum_loft_lofts l ON j.loft_id = l.id
        LEFT JOIN lum_loft_email_templates t ON j.template_id = t.id
        WHERE 1=1 AND j.id = 10
        ORDER BY j.created_at DESC
        LIMIT 200 made by do_action('lofts-1325_page_wp_loft_booking_email_jobs'), WP_Hook->do_action, WP_Hook->apply_filters, wp_loft_booking_email_jobs_page, wp_loft_booking_fetch_email_jobs
